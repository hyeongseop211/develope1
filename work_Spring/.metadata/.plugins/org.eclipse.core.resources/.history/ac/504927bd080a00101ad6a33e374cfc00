package spring_ex2_2;

public class Season {
	private int month;
	

	public int getMonth() {
		return month;
	}

	public void setMonth(int month) {
		this.month = month;
	}
	
	public void season() {
		switch ( month) {
		int month =0;
		case 3<=month<=5:
			System.out.println("봄");
			break;
		case 6<=month<=8:
			System.out.println("여름");
		break;
		case 9<=month<=11:
			System.out.println("가을");
		break;
		case 12=month && month <=2:
			System.out.println("겨울");
		break;

		default:
			break;
		}
	}
}
